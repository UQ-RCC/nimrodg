{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"identifier": {
			"type": "string",
			"pattern": "^[a-zA-Z0-9_]+$"
		},
		"variable_identifier": {
			"type": "string",
			"pattern": "^[a-zA-Z_][a-zA-Z0-9_]*$"
		},
		"config_key": {
			"type": "string",
			"pattern": "^[a-zA-Z0-9_]+(?:\\.[a-zA-Z0-9_]+)*"
		},
		"experiment_name": { "$ref": "#/definitions/identifier" },
		"resource_name": { "$ref": "#/definitions/identifier" }
	}
}
