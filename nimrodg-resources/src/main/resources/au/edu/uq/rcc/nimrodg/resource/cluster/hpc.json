{
  "pbspro": {
    "submit": ["qsub"],
    "delete": ["qdel"],
    "delete_force": ["qdel", "-W", "force"],
    "regex": "^(.+)$"
  },
  "slurm": {
    "submit": ["sbatch"],
    "delete": ["scancel", "-b"],
    "delete_force": ["scancel", "-f", "-s", "KILL"],
    "regex": "^.*?(\\d+).*$"
  },
  "lsf": {
    "submit": ["bsub", "-f"],
    "delete": ["bkill"],
    "delete_force": ["bkill", "-s", "KILL"],
    "regex": "^Job\\s+<([^>]+)>.*$"
  }
}

